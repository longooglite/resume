    "generate": "nuxt generate"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.11.3",
    "apexcharts": "^5.3.6",
    "moment": "^2.30.1",
    "nuxt": "4",
    "phosphor-vue": "^4.4.1",
    "pinia": "^3.0.4",
    "vue": "^3.4.0",
    "vue3-apexcharts": "^1.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.4.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "commitizen": "^4.3.0",
    "cypress": "^13.7.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "vite": "^5.0.0"
  },
  "lint-staged": {
    "*.{js,ts,vue,json,css,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
