{
  "name": "vue-resume",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "preview": "vite preview",
    "cy:open": "cypress open --component",
    "test:component": "cypress run --component",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
